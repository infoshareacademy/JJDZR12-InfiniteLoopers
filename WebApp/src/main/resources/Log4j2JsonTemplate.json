{
  "timestamp": "${date:yyyy-MM-dd'T'HH:mm:ss.SSSXXX}",
  "level": "${level}",
  "thread": "${threadName}",
  "logger": "${loggerName}",
  "message": "${message:json}",
  "exception": {
    "class": "${exception:className}",
    "message": "${exception:message}",
    "stackTrace": "${exception:stackTrace:text}"
  }
}
